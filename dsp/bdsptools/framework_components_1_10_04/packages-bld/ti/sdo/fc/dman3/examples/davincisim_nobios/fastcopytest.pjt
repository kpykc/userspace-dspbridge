[Project Settings]
ProjectName="DavinciDMATest"
ProjectDir="$(Proj_dir)"
ProjectType=Executable
CPUFamily=TMS320C64XX
Tool="Compiler"
Tool="CustomBuilder"
Tool="DspBiosBuilder"
Tool="Linker"
Config="Debug"
Config="Release"

[Project Dependencies]
Source="..\fcpy_ti64.pjt"

["..\fcpy_ti64.pjt" Settings]
MatchConfigName=true

[Source Files]
Source="..\bios_rts.c"
Source="..\bios_rtsasm.s62"
Source="..\fastcopytest.c"
Source="..\fcpy.c"
