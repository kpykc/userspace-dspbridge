["Compiler" Settings: "Release"]
Options=-o2 -eoo64Py -fr"$(Proj_dir)\Release" -i"." -i"%FC_INSTALL_DIR%/packages" -i"%XDAIS_INSTALL_DIR%/packages" -d"CHIP_6416" -d"NON_RTSC_CONFIGURATION" -d"_RTS_MODE" -d"IDMA3_USEFULLPACKAGEPATH" -ml3 -mv6400+ --no_compress --disable:sploop

["Linker" Settings: "Debug"]
Options=-q -c -m".\debug\fastcopytest.map" -o".\debug\fastcopytest.out" -x -i"%FC_INSTALL_DIR%/packages/ti/sdo/fc/acpy3" -i"%FC_INSTALL_DIR%/packages/ti/sdo/fc/dman3" -i"$(Install_dir)/c6000/xdais/lib" -l"acpy3.a64P" -l"dman3.a64P"

["Linker" Settings: "Release"]
